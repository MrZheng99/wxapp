.container{
    height: 100vh;
    background-color:#FFB6C1;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}
.detail{
    display: flex;
    flex-direction: column;
    margin: 10rpx 0rpx;
    
}
.bar{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    padding: 10rpx 0rpx;
    text-align: center;
}
.box{
    width: 33.3%;
    padding: 10rpx 0rpx;
    text-align: center
}
.weather{
    display: flex;
    flex-direction: row;
    justify-content: space-around
}
.icon{
    width:500rpx;
}
.tmp{
    font-size: 25rpx;
    flood-color: brown;
    display: flex;
    flex-direction: column;
    justify-content:space-around
}
